{
  "version": 2,
  "builds": [
    {
      "src": "main.jsx",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)", 
      "headers": { "Cache-Control": "max-age=31536000, public" },
      "status": 200,
      "continue": true
    },
    {
      "src": "/.*",
      "dest": "main.jsx"
    }
  ]
}